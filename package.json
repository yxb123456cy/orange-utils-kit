  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@arco-design/web-vue": "^2.57.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^10.0.0",
    "cz-git": "^1.12.0",
    "eslint": "^9.34.0",
    "husky": "^9.1.7",
    "rollup-plugin-visualizer": "^6.0.3",
    "standard-version": "^9.5.0",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.2",
